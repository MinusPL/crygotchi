; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Crygotchi"
config/description=" A casual room building, decoration and virtual pet game"
run/main_scene="res://scenes/main_scene.tscn"
config/features=PackedStringArray("4.0", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

DebugMenu="*res://addons/debug_menu/debug_menu.tscn"
TilesDatabase="*res://core/databases/TilesDatabase.cs"
ItemsDatabase="*res://core/databases/ItemsDatabase.cs"
SaveManager="*res://core/save/SaveManager.cs"
DialogueService="*res://components/dialogue/scripts/DialogueService.cs"
RoomState="*res://components/room/scripts/RoomState.cs"
CursorState="*res://components/cursor/scripts/CursorState.cs"
CryState="*res://components/hub/scripts/cry/CryState.cs"
Console="*res://addons/panku_console/console.tscn"

[display]

window/size/viewport_width=1440
window/size/viewport_height=810
window/size/mode=2
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="Crygotchi"

[editor_plugins]

enabled=PackedStringArray("res://addons/debug_menu/plugin.cfg", "res://addons/panku_console/plugin.cfg")

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

